<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <style>
        table 
        {
            width: 100%;
            border-collapse: collapse;
            border: 3px solid black;
        }

        td
        {
            border: 3px solid black;
            text-align: center;
            padding: 10px;
            font-size: 18px;
        }
    </style>
</head>
<body>
    <div class="number-table-container">
        <table border="1" cellpadding="10" cellspacing="10">
            <tr>
                <td colspan="2"><b>1 &nbsp; 2</b></td>
                @*<td><b>2</b></td>*@
                <td><b>3</b></td>
                <td><b>4</b></td>
                <td><b>5</b></td>
            </tr>
            <tr>
                <td>6</td>
                <td>7</td>
                <td>8</td>
                <td colspan="2" rowspan="2">9 &nbsp; 10 <br /> 14 &nbsp; 15</td>
                @*<td>10</td>*@
            </tr>
            <tr>
                <td>11</td>
                <td>12</td>
                <td>13</td>
                @*<td>14</td>*@
                @*<td>15</td>*@
            </tr>
            <tr>
                <td>16</td>
                <td>17</td>
                <td rowspan="2">18 <br /> 23</td>
                <td>19</td>
                <td>20</td>
            </tr>
            <tr>
                <td>21</td>
                <td>22</td>
                @*<td>23</td>*@
                <td>24</td>
                <td>25</td>
            </tr>
        </table>
    </div>
</body>
</html>
